{
  "name": "comms-agent-dev",
  "build": {
    "dockerfile": "../Dockerfile",
    "context": ".."
  },
  "postCreateCommand": "pip install -r requirements.txt",
  "features": {
    "ghcr.io/devcontainers/features/azure-cli:1": {}
  },
"customizations": {
